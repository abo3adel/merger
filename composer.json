{
    "name": "abo3adel\/merger",
    "description": "merge stub files with project predefined files",
    "keywords": [
        "laravel",
        "composer",
        "merger",
        "project scallfold",
        "replace predefined files",
        "replace laravel files",
        "env",
        "composer",
        "npm",
        "yarn"
    ],
    "homepage": "https://github.com/abo3adel/merger",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Adel",
            "email": "abo3adel35@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "digitickets/lalit": "^3.1",
        "symfony/yaml": "^5.2"
    },
    "require-dev": {
        "mockery\/mockery": "^1.4.2",
        "pestphp\/pest": "^1.0",
        "laravel-zero\/framework": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests\/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "merger"
    ]
}
